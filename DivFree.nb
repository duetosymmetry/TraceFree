Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"DefTensor", "::", "BadDivFreeIndices"}], "=", 
   "\"\<Indices supplied to DivFree (`1`) do not all appear with opposite \
character in indices of tensor (`2`).\>\""}], ";"}]], "Input",
 InitializationCell->True],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"CheckDivFree", "[", 
    RowBox[{
     RowBox[{"tensor_", "[", "inds___", "]"}], ",", 
     RowBox[{"df", ":", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"_", "?", "CovDQ"}], "[", "_", "]"}], "..."}], "}"}]}]}], 
    "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"dfinds", "=", 
       RowBox[{"Union", "[", 
        RowBox[{"First", "/@", "df"}], "]"}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"With", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"changedfinds", "=", 
          RowBox[{"ChangeIndex", "/@", "dfinds"}]}], "}"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Every", " ", "index", " ", "in", " ", "changedfinds", " ", "must", 
          " ", "be", " ", "in", " ", "inds"}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Intersection", "[", 
             RowBox[{
              RowBox[{"{", "inds", "}"}], ",", "changedfinds"}], "]"}], "=!=",
             "changedfinds"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"Throw", "@", 
             RowBox[{"Message", "[", 
              RowBox[{
               RowBox[{"DefTensor", "::", "BadDivFreeIndices"}], ",", 
               "dfinds", ",", 
               RowBox[{"{", "inds", "}"}]}], "]"}]}], ";"}]}], 
          "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", "]"}],
       ";"}]}], "\[IndentingNewLine]", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"CheckDivFree", "[", 
    RowBox[{"_", ",", "df_"}], "]"}], ":=", 
   RowBox[{"Throw", "@", 
    RowBox[{"Message", "[", 
     RowBox[{
      RowBox[{"DefTensor", "::", "invalid"}], ",", "df", ",", 
